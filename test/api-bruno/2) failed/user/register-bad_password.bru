meta {
  name: register-bad_password
  type: http
  seq: 3
}

post {
  url: {{http-s}}://{{host}}/user/register
  body: json
  auth: none
}

body:json {
  {
    "email": "user{{randomNumber}}@email.com",
    "password": "bad-password"
  }
}

assert {
  res.status: eq 400
  res.body: isString
  res.body: matches Invalid request
}
