meta {
  name: register-user_already_exist
  type: http
  seq: 1
}

post {
  url: {{http-s}}://{{host}}/user/register
  body: json
  auth: none
}

body:json {
  {
    "email": "user{{randomNumber}}@email.com",
    "password": "Qwerty_123"
  }
}

assert {
  res.status: eq 409
  res.body: isString
  res.body: matches User already exist
}
